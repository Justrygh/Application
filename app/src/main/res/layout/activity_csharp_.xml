<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".csharp_Activity">
    <ImageView
        android:id="@+id/home_page_B"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/home_page_back"
        app:layout_constraintBottom_toBottomOf="@+id/home_page_back"
        app:layout_constraintEnd_toEndOf="@+id/home_page_back"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/easy_study_back"
        android:layout_width="match_parent"
        android:layout_height="151dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/easystudy_icon" />

    <TextView
        android:id="@+id/csh_if_else"
        android:layout_width="127dp"
        android:layout_height="47dp"
        android:text="if ... Else"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.088"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/csh_string"
        app:layout_constraintVertical_bias="0.075" />

    <TextView
        android:id="@+id/csh_string"
        android:layout_width="127dp"
        android:layout_height="47dp"
        android:text="Strings"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.088"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/csh_var"
        app:layout_constraintVertical_bias="0.061" />

    <TextView
        android:id="@+id/csh_var"
        android:layout_width="127dp"
        android:layout_height="47dp"
        android:layout_marginTop="36dp"
        android:text="variables"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.088"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/easy_study_back"
        app:layout_constraintVertical_bias="0.016" />

    <CheckBox
        android:id="@+id/vars_checkBox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:layout_marginBottom="336dp"
        android:text="Done"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.85"
        app:layout_constraintStart_toEndOf="@+id/csh_var"
        app:layout_constraintTop_toBottomOf="@+id/easy_study_back"
        app:layout_constraintVertical_bias="0.089" />

    <CheckBox
        android:id="@+id/if_else_checkBox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Done"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.85"
        app:layout_constraintStart_toEndOf="@+id/csh_if_else"
        app:layout_constraintTop_toBottomOf="@+id/string_checkBox"
        app:layout_constraintVertical_bias="0.12" />

    <CheckBox
        android:id="@+id/string_checkBox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Done"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.845"
        app:layout_constraintStart_toEndOf="@+id/csh_string"
        app:layout_constraintTop_toBottomOf="@+id/vars_checkBox"
        app:layout_constraintVertical_bias="0.088" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="match_parent"
        android:layout_height="127dp"
        app:layout_constraintBottom_toBottomOf="@+id/home_page_B"
        app:layout_constraintEnd_toEndOf="@+id/home_page_B"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/home_page_B"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/easystudy" />

</androidx.constraintlayout.widget.ConstraintLayout>